#!/bin/bash
if [ $# -eq 0 ] ;then
	echo "No argument supplied"
elif [ $# -eq 1 ] ;then
	echo "Only one argument supplied"
else
	if [ $1 -gt $2 ]
	then
		echo "The first number is greater than the second"
	elif [ $1 -lt $2 ]
	then
		echo "The second number is greater than the first"
	else
		echo "The 2 numbers are equal"
	fi
fi
